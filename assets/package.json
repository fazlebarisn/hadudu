{
  "name": "hadudu",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
    "clean": "rm -rf build/*",
    "svg": "svgr -d src/js/icons src/icons",
    "lint": "eslint src/js/*",
    "lint:fix": "eslint --fix src/js/*",
    "lint-css": "stylelint 'src/sass/**/*.scss'",
    "lint-css:fix": "npm run lint-css -- --fix",
    "precommit": "npm run lint-css:fix && npm run lint:fix && npm run prod"
  },
  "keywords": [
    "wordpress",
    "themes"
  ],
  "author": "Fazle Bari",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "style-loader": "^3.3.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
